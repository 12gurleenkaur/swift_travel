{
  "search.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.xcbuild": true
    // "**/*.g.dart": true,
    // "**/*.freezed.dart": true,
  },
  "[dart]": {
    "editor.rulers": [
      100
    ],
  },
  "dart.lineLength": 100,
  "dart.flutterTestAdditionalArgs": [
    "--no-sound-null-safety"
  ],
  "dart.flutterRunAdditionalArgs": [
    "--no-sound-null-safety"
  ],
}